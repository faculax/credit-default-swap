  <!-- USD Discount Curve -->
  <YieldCurve>
    <CurveId>USD-DISCOUNT</CurveId>
    <CurveDescription>USD Discount Curve</CurveDescription>
    <Currency>USD</Currency>
    <DiscountCurve>USD-DISCOUNT</DiscountCurve>
    <Segments>
      <Direct>
        <Type>Zero</Type>
        <Quotes>
          <Quote>DISCOUNT/USD/0/1M</Quote>
          <Quote>DISCOUNT/USD/0/3M</Quote>
          <Quote>DISCOUNT/USD/0/6M</Quote>
          <Quote>DISCOUNT/USD/0/1Y</Quote>
          <Quote>DISCOUNT/USD/0/2Y</Quote>
          <Quote>DISCOUNT/USD/0/3Y</Quote>
          <Quote>DISCOUNT/USD/0/5Y</Quote>
