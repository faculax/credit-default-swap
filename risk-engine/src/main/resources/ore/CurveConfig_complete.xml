          <Quote>DISCOUNT/USD/0/15Y</Quote>
          <Quote>DISCOUNT/USD/0/20Y</Quote>
          <Quote>DISCOUNT/USD/0/30Y</Quote>
        </Quotes>
        <Conventions>USD-ZERO-CONVENTIONS</Conventions>
      </Direct>
    </Segments>
    <InterpolationVariable>Zero</InterpolationVariable>
    <InterpolationMethod>LogLinear</InterpolationMethod>
    <YieldCurveDayCounter>ACT/365</YieldCurveDayCounter>
    <Tolerance>0.000001</Tolerance>
